        <script src="/filepond/filepond.min.js"></script>
        <script src="/filepond/filepond-plugin-file-validate-type.min.js"></script>
        <script src="/filepond/filepond-plugin-image-resize.min.js"></script>
        <script src="/filepond/filepond-plugin-image-transform.min.js"></script>
        <script src="/filepond/filepond-plugin-image-preview.min.js"></script>
        <style><%= render "src/views/pages/ktistec.css.ecr" %></style>
        <script><%= render "src/views/pages/ktistec.js.ecr" %></script>
